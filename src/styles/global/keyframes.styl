@keyframes shake {
  0% {
    transform: translateX(0%);
  }
  50% {
    transform: translateX(5%);
  }
  100% {
    transform: translateX(-5%);
  }
}

@keyframes stripped {
  0% {
    background-position: 0 0;
  }

  100% {
    background-position: 30px 60px;
  }
}

@keyframes bounce-in {
  0% {
    transform: scale(0.50);
  }
  50% {
    transform: scale(1.02);
  }
  100% {
    transform: scale(1);
  }
}
