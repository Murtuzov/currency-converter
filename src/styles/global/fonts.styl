
@font-face {
  font-family: 'IBM Plex Sans';
  src: url('/public/fonts/IBMPlexSans/IBMPlexSans-Bold.woff2') format('woff2'),
    url('/public/fonts/IBMPlexSans/IBMPlexSans-Bold.woff') format('woff');
  font-weight: 700; //bold
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'IBM Plex Sans';
  src: url('/public/fonts/IBMPlexSans/IBMPlexSans-Medium.woff2') format('woff2'),
    url('/public/fonts/IBMPlexSans/IBMPlexSans-Medium.woff') format('woff');
  font-weight: 500;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'IBM Plex Sans';
  src: url('/public/fonts/IBMPlexSans/IBMPlexSans-Regular.woff2') format('woff2'),
    url('/public/fonts/IBMPlexSans/IBMPlexSans-Regular.woff') format('woff');
  font-weight: 400; //normal
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'IBM Plex Sans';
  src: url('/public/fonts/IBMPlexSans/IBMPlexSans-SemiBold.woff2') format('woff2'),
    url('/public/fonts/IBMPlexSans/IBMPlexSans-SemiBold.woff') format('woff');
  font-weight: 600;
  font-style: normal;
  font-display: swap;
}

